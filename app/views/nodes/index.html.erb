<p id="notice"><%= notice %></p>

<h1>Listing Nodes</h1>

<table>
  <thead>
    <tr>
      <th>Track</th>
      <th>Time</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Alt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nodes.each do |node| %>
      <tr>
        <td><%= node.track_id %></td>
        <td><%= node.time %></td>
        <td><%= node.lat %></td>
        <td><%= node.long %></td>
        <td><%= node.alt %></td>
        <td><%= link_to 'Show', node %></td>
        <td><%= link_to 'Edit', edit_node_path(node) %></td>
        <td><%= link_to 'Destroy', node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Node', new_node_path %>
